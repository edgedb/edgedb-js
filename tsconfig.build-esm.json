{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "es2015",
    "outDir": "./dist/__esm",
    "declaration": false,
    "declarationDir": null,
    "paths": {
      "edgedb": [
        "./src/index.node"
      ],
      "@generated/*": [
        "./src/syntax/genMock/*"
      ],
    }
  },
  "include": [
    "src/syntax",
  ],
  "exclude": [
    "**/*.deno.ts",
    "test/deno/*",
    "dist",
    "qb",
  ]
}
