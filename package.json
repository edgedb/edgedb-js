{
  "name": "edgedb",
  "description": "A JavaScript binding for EdgeDB",
  "homepage": "https://edgedb.com/docs",
  "author": "EdgeDB <info@edgedb.com>",
  "engines": {
    "node": ">= 10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/edgedb/edgedb-js.git"
  },
  "version": "0.15.0-alpha.0",
  "main": "./dist/src/index.node.js",
  "types": "./dist/src/index.node.d.ts",
  "license": "Apache-2.0",
  "files": [
    "/dist"
  ],
  "browser": {
    "./dist/src/index.node.js": "./dist/src/index.browser.js"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^13.1.6",
    "get-stdin": "^7.0.0",
    "in-publish": "^2.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "prettylint": "^1.0.0",
    "proposal-temporal": "^0.7.0",
    "ts-jest": "^26.5.2",
    "ts-node": "^10.0.0",
    "tslint": "^5.20.1",
    "typescript": "4.3.5"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json --incremental",
    "test": "jest --detectOpenHandles",
    "lint": "tsc && tslint 'src/**/*.ts' && prettylint 'src/**/*.ts' 'test/**/*.ts'",
    "lint:fix": "tslint --fix 'src/**/*.ts' 'qb/**/*.ts' --exclude '**/node_modules/**'",
    "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "gen-errors": "edb gen-errors-json --client | node tools/gen_errors.js",
    "prepublish": "in-publish && yarn lint && yarn build && yarn compileForDeno && yarn test || not-in-publish",
    "compileForDeno": "deno run --unstable --allow-env --allow-read --allow-write tools/compileForDeno.ts",
    "generate": "cd qb && ts-node --project ../tsconfig.json run.ts && prettier --write './**/*.ts'",
    "generate:dev": "cd qb && nodemon -e ts -w ../src -x ts-node --project ../tsconfig.json run.ts",
    "build:dev": "nodemon -e ts -w ./src -w ./qb -x yarn build --incremental"
  }
}
