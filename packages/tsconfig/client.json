{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Client",
  "compilerOptions": {
    "lib": ["es2022"],
    "target": "es2022",
    "module": "Node16",
    "moduleResolution": "Node16",
    "declaration": true,
    "strict": true,
    "downlevelIteration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true
  }
}
