{
  "compilerOptions": {
    "lib": [
      "ESNext"
    ],
    "module": "CommonJS",
    "moduleResolution": "node",
    "strict": true,
    "downlevelIteration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true
  },
}
