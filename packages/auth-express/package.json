{
  "name": "@gel/auth-express",
  "description": "Helper library to integrate the Gel Auth extension with Express",
  "version": "0.3.0-rc.1",
  "type": "module",
  "author": "Gel <info@geldata.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/geldata/gel-js.git",
    "directory": "packages/auth-express"
  },
  "license": "Apache-2.0",
