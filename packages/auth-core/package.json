    "lint:fix": "eslint --fix",
    "gen-consts": "node genConsts.js"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.13",
    "jest": "29.7.0",
    "ts-jest": "29.1.4",
    "typescript": "^5.5.2",
    "gel": "^2.0.0-rc",
    "@repo/tsconfig": "*"
  },
