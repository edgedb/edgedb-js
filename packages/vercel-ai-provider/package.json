{
  "name": "@gel/vercel-ai-provider",
  "description": "Provider for the Vercel AI SDK",
  "version": "0.1.0-rc.1",
  "license": "Apache-2.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/geldata/gel-js.git",
    "directory": "packages/vercel-ai-provider"
  },
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "jest --detectOpenHandles --passWithNoTests",
    "build": "tsc --project tsconfig.json",
    "lint": "eslint --quiet",
    "lint:fix": "eslint --fix"
  },
  "dependencies": {
    "@ai-sdk/provider": "^1.0.0",
    "@ai-sdk/provider-utils": "^2.0.0"
  },
  "devDependencies": {
    "@repo/tsconfig": "*",
    "@types/node": "^18",
    "tsup": "^8",
    "typescript": "5.5.4",
    "zod": "3.23.8",
    "gel": "^2.0.0-rc"
  },
  "peerDependencies": {
    "zod": "^3.0.0",
    "gel": "^2.0.0-rc"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "ai"
  ]
}
