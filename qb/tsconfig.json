{
  "compilerOptions": {
    "lib": [
      "es5",
      "es6",
      "es7",
      "esnext",
      "dom"
    ],
    "target": "es5",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "strict": true,
    "strictPropertyInitialization": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "downlevelIteration": true,
    "isolatedModules": true,
    "baseUrl": ".",
    "paths": {
      "edgedb": [
        "../src/index.node.ts"
      ]
    }
  },
  "include": [
    ".",
  ],
}
