{
  "compilerOptions": {
    "target": "es2019",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "strict": true,
    "strictPropertyInitialization": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "downlevelIteration": true,
    "isolatedModules": true,
    "baseUrl": ".",
    "outDir": "dist",
    // "declaration": true,
    "paths": {
      // "edgedb": [
      //   "../src/index.node.ts"
      // ],
      // "edgedb/*": [
      //   "../*"
      // ],
      // "reflection": [
      //   "../src/reflection"
      // ],
      "@syntax/*": [
        "../src/syntax/*"
      ],
      "@generated/*": [
        "./generated/example/*"
      ],
    }
  },
  "include": [
    ".",
  ],
}
