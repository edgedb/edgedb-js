{
  "compilerOptions": {
    "lib": [
      "dom",
      "es6"
    ],
    "target": "es3",
    "module": "commonjs",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "strict": true,
    "strictPropertyInitialization": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    // "downlevelIteration": true,
    "isolatedModules": true,
    "baseUrl": ".",
    "declaration": true,
    "preserveSymlinks": true,
    "paths": {}
  },
  "include": [
    ".",
  ],
}
